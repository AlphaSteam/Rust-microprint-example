{
    "scale": 2,
    "vertical_spacing": 1.4,
    "microprint_width": 140,
    "default_colors": {
        "background_color": "rgb(30, 30, 30)",
        "text_color": "white"
    },
    "line_rules": [
        {
            "includes": [
                "(?:^|\\W)error(?:$|\\W)(?!code)",
                "panicked",
                "failed",
                "stacktrace"
            ],
            "excludes": [
                "checking",
                "compiling",
                "(?:^|\\W)0(?:$|\\W)",
                "info"
            ],
            "text_color": "white",
            "background_color": "#910404"
        },
        {
            "includes": [
                "(?:^|\\W)warning(?:$|\\W)"
            ],
            "text_color": "black",
            "background_color": "#dfdf07"
        },
        {
            "includes": [
                "(?:^|\\W)test result(?:$|\\W)"
            ],
            "text_color": "black",
            "background_color": "#d0e6c9"
        },
        {
            "includes": [
                "(?:^|\\W)successfully(?:$|\\W)"
            ],
            "text_color": "white",
            "background_color": "rgb(38, 162, 105)"
        }
    ],
    "font-family": "monospace, monospace"
}